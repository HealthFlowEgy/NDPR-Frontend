{
  "expo": {
    "name": "HealthFlow",
    "slug": "healthflow-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healthflow.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.healthflow.mobile"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow HealthFlow to access your camera for QR code scanning."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3498db"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow HealthFlow to use Face ID for authentication."
        }
      ]
    ],
    "scheme": "healthflow",
    "extra": {
      "eas": {
        "projectId": "1def6c0c-9145-45c0-9c0b-6b1698a676a7"
      }
    },
    "owner": "healthflow"
  }
}
