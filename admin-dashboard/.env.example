# HealthFlow Admin Dashboard Environment Configuration
# Copy this file to .env and update values as needed

# API Configuration
REACT_APP_API_URL=https://api.healthflow.tech/api/v1

# Keycloak Configuration (following Sunbird RC best practices)
REACT_APP_KEYCLOAK_URL=https://keycloak.healthflow.tech
REACT_APP_KEYCLOAK_REALM=RegistryAdmin
REACT_APP_KEYCLOAK_CLIENT_ID=admin-portal

# WebSocket Configuration (for real-time notifications)
REACT_APP_SOCKET_URL=wss://api.healthflow.tech/ws
